<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="CustomerDetails" table="CustomerDetails">
    <id name="Id">
      <column name="Id" sql-type="int" not-null="true" />
      <generator class="identity" />
    </id>
    <property name="CustomerId">
      <column name="CustomerId" />
    </property>
    <property name="FeatureId">
      <column name="FeatureId" />
    </property>
    <property name="FeatureValue">
      <column name="FeatureValue" />
    </property>
    <property name="Description">
      <column name="Description" />
    </property>
  </class>
</hibernate-mapping>