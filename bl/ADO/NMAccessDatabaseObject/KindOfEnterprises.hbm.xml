<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="KindOfEnterprises" table="KindOfEnterprises">
    <id name="Id">
      <column name="Id" sql-type="String" not-null="true" length="20" />
      <generator class="assigned" />
    </id>
    <property name="Name">
      <column name="Name" />
    </property>
    <property name="Description">
      <column name="Description" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" />
    </property>
    <property name="CreatedBy">
      <column name="CreatedBy" />
    </property>
    <set name="CustomersList" lazy="true" cascade="delete">
      <key column="KindOfEnterpriseId" />
      <one-to-many class="Customers" />
    </set>
  </class>
</hibernate-mapping>