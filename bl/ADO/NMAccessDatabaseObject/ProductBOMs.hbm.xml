<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="ProductBOMs" table="ProductBOMs">
    <id name="Id">
      <column name="Id" sql-type="int" not-null="true" />
      <generator class="identity" />
    </id>
    <property name="ParentId">
      <column name="ParentId" />
    </property>
    <property name="ProductId">
      <column name="ProductId" />
    </property>
    <property name="Quantity">
      <column name="Quantity" />
    </property>
	  <property name="LossRate">
		  <column name="LossRate" />
	  </property>
	  <property name="ReplacementTime">
		  <column name="ReplacementTime" />
	  </property>
	  <property name="Description">
      <column name="Description" />
    </property>
    <property name="ValidFrom">
      <column name="ValidFrom" />
    </property>
    <property name="ValidUntil">
      <column name="ValidUntil" />
    </property>
  </class>
</hibernate-mapping>