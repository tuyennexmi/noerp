<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="ExportDetails" table="ExportDetails">
    <id name="OrdinalNumber">
      <column name="OrdinalNumber" sql-type="int" not-null="true" />
      <generator class="identity" />
    </id>
    <property name="ExportId">
      <column name="ExportId" />
    </property>
    <property name="ProductId">
      <column name="ProductId" />
    </property>
	  <property name="UnitId">
		  <column name="UnitId" />
	  </property>
    <property name="StockId">
      <column name="StockId" />
    </property>
    <property name="RequiredQuantity">
      <column name="RequiredQuantity" />
    </property>
    <property name="Quantity">
      <column name="Quantity" />
    </property>
	  <property name="Price">
		  <column name="Price" />
	  </property>
	  <property name="Amount">
		  <column name="Amount" />
	  </property>
	  <property name="Tax">
		  <column name="Tax" />
	  </property>
	  <property name="TaxAmount">
		  <column name="TaxAmount" />
	  </property>
	  <!--<property name="ExportTaxRate">
		  <column name="ExportTaxRate" />
	  </property>
	  <property name="ExportTaxAmount">
		  <column name="ExportTaxAmount" />
	  </property>-->
	  <property name="Discount">
		  <column name="Discount" />
	  </property>
	  <property name="DiscountAmount">
		  <column name="DiscountAmount" />
	  </property>
	  <property name="TotalAmount">
		  <column name="TotalAmount" />
	  </property>
	  
    <property name="Description">
      <column name="Description" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" />
    </property>
    <property name="CreatedBy">
      <column name="CreatedBy" />
    </property>
	  <property name="ModifiedDate">
		  <column name="ModifiedDate" />
	  </property>
	  <property name="ModifiedBy">
		  <column name="ModifiedBy" />
	  </property>
  </class>
</hibernate-mapping>