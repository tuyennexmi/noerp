<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="TaskDetails" table="TaskDetails">
    <id name="OrdinalNumber">
      <column name="OrdinalNumber" sql-type="int" not-null="true" />
      <generator class="identity" />
    </id>
    <property name="No">
      <column name="No" />
    </property>
	  <property name="TaskId">
		  <column name="TaskId" />
	  </property>
	  <property name="Name">
		  <column name="Name" />
	  </property>
    <property name="UserId">
      <column name="UserId" />
    </property>
    <property name="StartDate">
      <column name="StartDate" />
    </property>
	  <property name="EndDate">
		  <column name="EndDate" />
	  </property>
	  <property name="TimeSpent">
		  <column name="TimeSpent" />
	  </property>
	  <property name="Purpose">
		  <column name="Purpose" />
	  </property>
	  <property name="WorkSummary">
		  <column name="WorkSummary" />
	  </property>
	  <property name="Criteria">
		  <column name="Criteria" />
	  </property>
	  <property name="CheckedBy">
		  <column name="CheckedBy" />
	  </property>
	  <property name="Manager">
		  <column name="Manager" />
	  </property>
    <property name="CreatedDate">
      <column name="CreatedDate" />
    </property>
    <property name="CreatedBy">
      <column name="CreatedBy" />
    </property>
	  <property name="ModifiedDate">
		  <column name="ModifiedDate" />
	  </property>
	  <property name="ModifiedBy">
		  <column name="ModifiedBy" />
	  </property>
  </class>
</hibernate-mapping>