<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="Jobs" table="Jobs">
    <id name="Id">
		<column name="Id" sql-type="String" not-null="true" length="30" />
		<generator class="assigned" />
    </id>
	  <property name="Name">
		  <column name="Name" />
	  </property>
	  <property name="Purpose">
		  <column name="Purpose" />
	  </property>
	  <property name="WorkSummary">
		  <column name="WorkSummary" />
	  </property>
	  <property name="Criteria">
		  <column name="Criteria" />
	  </property>
	  <property name="WorkGuids">
		  <column name="WorkGuids" />
	  </property>
	  <property name="Tags">
		  <column name="Tags" />
	  </property>
	  <property name="TimeSpent">
		  <column name="TimeSpent" />
	  </property>
	  <property name="Status">
		  <column name="Status" />
	  </property>
    <property name="CreatedDate">
      <column name="CreatedDate" />
    </property>
    <property name="CreatedBy">
      <column name="CreatedBy" />
    </property>
	  <property name="ModifiedDate">
		  <column name="ModifiedDate" />
	  </property>
	  <property name="ModifiedBy">
		  <column name="ModifiedBy" />
	  </property>
  </class>
</hibernate-mapping>