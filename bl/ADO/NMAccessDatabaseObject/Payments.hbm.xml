<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="Payments" table="Payments">
    <id name="PaymentId">
      <column name="PaymentId" sql-type="varchar" not-null="true" length="20" />
      <generator class="assigned" />
    </id>
    <property name="PaymentBatchId">
      <column name="PaymentBatchId" />
    </property>
    <property name="PaymentDate">
      <column name="PaymentDate" />
    </property>
    <property name="PaymentTypeId">
      <column name="PaymentTypeId" />
    </property>
    <property name="SupplierId">
      <column name="SupplierId" />
    </property>
    <property name="InvoiceId">
      <column name="InvoiceId" />
    </property>
	  <property name="Reference">
		  <column name="Reference" />
	  </property>
	  <property name="StockId">
		  <column name="StockId" />
	  </property>
    <property name="Amount">
      <column name="Amount" />
    </property>
    <property name="CurrencyId">
      <column name="CurrencyId" />
    </property>
    <property name="ExchangeRate">
      <column name="ExchangeRate" />
    </property>
    <property name="PaymentAmount">
      <column name="PaymentAmount" />
    </property>
    <property name="DescriptionInVietnamese">
      <column name="DescriptionInVietnamese" />
    </property>
    <property name="PaymentStatus">
      <column name="PaymentStatus" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" />
    </property>
    <property name="CreatedBy">
      <column name="CreatedBy" />
    </property>
	  <property name="ModifiedDate">
		  <column name="ModifiedDate" />
	  </property>
	  <property name="ModifiedBy">
		  <column name="ModifiedBy" />
	  </property>
    <property name="PaymentMethodId">
      <column name="PaymentMethodId" />
    </property>
	  <property name="ApprovedBy">
		  <column name="ApprovedBy" />
	  </property>
	  <property name="PaymentBankAccount">
		  <column name="PaymentBankAccount" />
	  </property>
	  <property name="ReceiveBank">
		  <column name="ReceiveBank" />
	  </property>
  </class>
</hibernate-mapping>