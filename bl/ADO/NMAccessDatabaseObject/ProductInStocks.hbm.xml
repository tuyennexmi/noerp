<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="ProductInStocks" table="ProductInStocks">
    <id name="OrdinalNumber">
      <column name="OrdinalNumber" sql-type="int" not-null="true" />
      <generator class="identity" />
    </id>
    <property name="ProductId">
      <column name="ProductId" />
    </property>
    <property name="StockId">
      <column name="StockId" />
    </property>
	  <property name="BeginQuantity">
		  <column name="BeginQuantity" />
	  </property>
	  <property name="ImportQuantity">
		  <column name="ImportQuantity" />
	  </property>
	  <property name="ExportQuantity">
		  <column name="ExportQuantity" />
	  </property>
    
    <property name="BadProductInStock">
      <column name="BadProductInStock" />
    </property>
    <property name="HoldQuantity">
      <column name="HoldQuantity" />
    </property>
	  <property name="MinQuantity">
		  <column name="MinQuantity" />
	  </property>
	  <property name="MaxQuantity">
		  <column name="MaxQuantity" />
	  </property>
	  
	  <property name="Discount">
		  <column name="Discount" />
	  </property>	  
    <property name="CostPrice">
      <column name="CostPrice" />
    </property>
	  <property name="SalesPrice">
		  <column name="SalesPrice" />
	  </property>
	  
    <property name="CreatedDate">
      <column name="CreatedDate" />
    </property>
    <property name="CreatedBy">
      <column name="CreatedBy" />
    </property>
	  <property name="ModifiedDate">
		  <column name="ModifiedDate" />
	  </property>
	  <property name="ModifiedBy">
		  <column name="ModifiedBy" />
	  </property>
  </class>
</hibernate-mapping>