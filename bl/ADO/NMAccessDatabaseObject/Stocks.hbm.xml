<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="NEXMI" assembly="NEXMI">
  <class name="Stocks" table="Stocks">
    <id name="Id">
      <column name="Id" sql-type="String" not-null="true" length="30" />
      <generator class="assigned" />
    </id>
    <property name="Name">
      <column name="Name" />
    </property>
    <property name="Address">
      <column name="Address" />
    </property>
	<property name="Telephone">
	  <column name="Telephone" />
	</property>
	<property name="Logo">
	  <column name="Logo" />
	</property>
	<property name="StockType">
      <column name="StockType" />
    </property>
    <property name="CreatedDate">
      <column name="CreatedDate" />
    </property>
    <property name="CreatedBy">
      <column name="CreatedBy" />
    </property>
    <property name="Highlight">
      <column name="Highlight" />
    </property>
  </class>
</hibernate-mapping>